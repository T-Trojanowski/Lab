class Account:
    """
    Class showing details for an account object
    """

    def __init__(self, name: str) -> None:
        """
        Constructor to create initial state of account object
        :param account_name: name of the account
        :param account_balance: balance of the account

        """
        self.__account_name = name
        self.__account_balance = 0


    def deposit(self, amount: float) -> bool:
        """
        Method to deposit into the account
        :param amount: Amount attempting to be added to the account_balance variable
        :return: True if account balance is changed, False if not
        """
        if amount > 0:
            self.__account_balance += amount
            return True
        else:
            return False


    def withdraw(self, amount: float) -> bool:
        """
        Method to withdraw from the account
        :param amount: Amount attempting to be removed from the account_balance variable
        :return:  True is account balance is changed, False if not
        """
        if amount > 0 and amount < self.__account_balance:
            self.__account_balance -= amount
            return True
        else:
            return False


    def get_balance(self):
        """
        Method to access balance of the account
        :return: Balance of the account
        """
        return self.__account_balance

    def get_name(self):
        """
        Method to access name of the account
        :return: Name of the account
        """
        return self.__account_name
